# scalaenv/sbtenv configuration
export SCALAENV_ROOT=/opt/boxen/homebrew/var/scalaenv
export SBTENV_ROOT=/opt/boxen/homebrew/var/sbtenv

if which scalaenv > /dev/null; then
  eval "$(scalaenv init -)";
fi

if which sbtenv > /dev/null; then
  eval "$(sbtenv init -)";
fi

